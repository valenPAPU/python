import math
Pi=3.1416
print("Ingrese valores de los puntos x1 ,y1 y z1")
x1 = float( input("X1: "))
y1 = float( input("Y1: "))
z1 = float( input("Z1: "))
print ("-----------------------------------------------------------------")
print("Ingrese valores de los puntos x2 ,y2 y z2")
x2 = float( input("X2: "))
y2 = float( input("Y2: "))
z2 = float( input("Z2: "))

distancia = ( (z2-z1)**2 + (y2-y1)**2 + (x2-x1)**2 )**0.5

print("la distancia sera", distancia)




