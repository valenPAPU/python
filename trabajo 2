#Decoraci√≤n: Nombre del Algoritmo
print("----------------------------------")
print("Ejercicio 2: PROMEDIO DE 3 NOTAS.")
print("----------------------------------")

#Entradas
print("ingrese las 3 notas del alumno N1,N2,N3")
N1 = int(input("N1:"))
N2 = int(input("N2:"))
N3 = int(input("N3:"))

#Proceso
P=int((N1+N2+N3)/3)

#Salida
print("Salida: ")
print(P)
